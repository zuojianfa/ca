fn main() {
    let i = 0;
    while (i < 10) {
        print i; print ' ';
        i += 1 + 2;
    }
    print '\n';

    while (i > 0) {
        print i; print ' ';
        i -= 1 + 2;
    }
    print '\n';

    i = 1;
    while (i < 100) {
        print i; print ' ';
        i *= 2 + 1;
    }
    print '\n';

    while (i > 0) {
        print i; print ' ';
        i /= 2 + 2;
    }
    print '\n';

    i = 100;
    i %= 3;
    print i; print ' ';
    let j = 1;
    while (j < 100) {
        i = 100;
        i %= j;
        j += 100 - 98 + 3;
        print i; print ' ';
    }
    print '\n';
}

