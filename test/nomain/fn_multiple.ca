fn add(v1: i32, v2: i32) -> i32 {
    return v1 + v2;
}

fn sub(v1: i64, v2: i64) -> i64 {
    return v1 - v2;
}

fn mul(v1: f32, v2: f32) -> f32 {
    return v1 * v2;
}

fn div(v1: f64, v2: f64) -> f64 {
    return v1 / v2;
}

let a = add(3, 4);
let b = sub(19, 3);
let m = mul(a as f32, b as f32);
let d = div(b as f64, a as f64);

print a; print '\n';
print b; print '\n';
print m; print '\n';
print d; print '\n';

