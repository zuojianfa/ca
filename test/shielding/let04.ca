fn main() {
    struct BB (i32, i32, char, f64, char);

    let BB(b1, b2, b3, b4, b5) = __zero_init__;
    print b1; print ' ';
    print b2; print ' ';
    b3 = 'C'; b5 = 'A';
    print b3; print ' ';
    print b4; print ' ';
    print b5; print '\n';

    let BB(b1, b2, b3, b4, ..) = __zero_init__;
    print b1; print ' ';
    print b2; print ' ';
    b3 = 'D';
    print b3; print ' ';
    print b4; print '\n';

    let BB(b1, b2, ..) = __zero_init__;
    print b1; print ' ';
    print b2; print '\n';

    let BB(b1, .., b2) = __zero_init__;
    b2 = 'B';
    print b1; print ' ';
    print b2; print '\n';

    let BB(.., b1, b2) = __zero_init__;
    b2 = 'Q';
    print b1; print ' ';
    print b2; print ' ';

    let BB(.., b1) = __zero_init__;
    b1 = 'P';
    print b1; print ' ';

    let BB(b1,..) = __zero_init__;
    print b1; print '\n';

    let b1@BB(..) = __zero_init__;
    b1.2 = 'R'; b1.4 = 'S';
    print b1; print ' ';
}

